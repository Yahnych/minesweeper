{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
        "browser": true
    },
    "globals": {
        "Minesweeper": true
    },
    "rules": {
      "quotes": [
        "error",
        "double"
      ],
      "indent": [
        "error",
        4
      ],
      "semi": [
        "error",
        "always"
      ],
      "strict": "off",
      "prefer-template": "off",
      "arrow-parens": "off",
      "no-mixed-operators": "off",
      "function-paren-newline": "off",
      "no-bitwise": "off"
    }
}